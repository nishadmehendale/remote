import java.time.*;
public class Date {
	private int d,m,y;
	
	
	public Date(int d, int m, int y)
	{
		this.d = d;
		this.m = m;
		this.y = y;
	}
	@Override
	public String toString() {
		return "Date " + this.d + " / " + this.m +  " / " + this.y + "";
	}
	
	public boolean isSmaller(Date d) {
		boolean flag;
		if(this.y == d.y)
		{
			if(this.m == d.m)
			{
				if(this.d == d.d)
				{
					flag = true;
				}
				else if(this.d < d.d)
				{
					flag = false;
				}
				else
				{
					flag = true;
				}
			}
			else if(this.m < d.m)
			{
				flag = false;
			}
			else{
				flag = true;
			}
		}
		else if (this.y < d.y)
		{
			flag = false;
		}
		else
		{
			flag = true;
		}
		return flag;
		}
	public int[] diff(Date d) {
		int []date= new int[3];
		LocalDate d1,d2,d3= LocalDate.now();
		d1 =LocalDate.of(this.y, this.m, this.d);
		d2 =LocalDate.of(d.y, d.m, d.d);
		Period p = Period.between(this,d);
		return date;
	}
	public int getD() {
		return d;
	}
	public int getM() {
		return m;
	}
	public int getY() {
		return y;
	}
	
}
