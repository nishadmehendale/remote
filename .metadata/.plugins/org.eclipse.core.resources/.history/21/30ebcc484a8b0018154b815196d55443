import java.util.*;
class palsearchsort {
	public static void main(String args[])
	{
		Scanner sc = new Scanner(System.in);
		String []str = new String[10];
		int loc[] = new int[10];
		int size[] = new int [10];
		int id = 0;
		int temp = 0;
		for(int i=0;i<10;i++)
		{
			System.out.println("Enter a string");
			str[i] = sc.nextLine();
		}
		for(int i=0;i<10;i++)
		{
			StringBuffer s = new StringBuffer(str[i]);
			System.out.println(s);
			
			if(s.equals(s.reverse()))
			{
				loc[id]=i;
				id+=1;
			}
		}
		for(int i=0;i<loc.length;i++)
		{
			size[i] = str[loc[i]].length();
		}
		for(int i=0;i<size.length;i++)
		{
			for(int j=0;j<size.length-1;j++)
			{
				if(size[j]<size[j+1])
				{
					temp = size[j];
					size[j] = size[j+1];
					size[j+1] = temp;
					temp = loc[j];
					loc[j] = loc[j+1];
					loc[j+1] = temp;
				}
		}
		}
		//System.out.println("Palindrome Strings are");
		for(int i=0;i<loc.length;i++)
		{
			//System.out.println(str[loc[i]]);
		}
	}
}
