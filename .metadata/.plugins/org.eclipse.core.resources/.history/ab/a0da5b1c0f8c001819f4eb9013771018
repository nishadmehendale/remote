
public class Time {
	private int hours;
	private int minutes;
	public Distance addDistance(Distance D1,Distance D2) {
		Distance D3 = new Distance();
		D3.inches = D1.getInches() + D2.getInches();
		D3.feet = D1.getFeet() + D2.getFeet();
		if(D3.inches >= 12.0)
			{
				D3.feet += 1;
				D3.inches -= 12.0;
			}
		
		return D3;
}
